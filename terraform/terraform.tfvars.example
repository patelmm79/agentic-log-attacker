# Terraform Variables - Example Configuration
# Copy this file to terraform.tfvars and fill in your actual values

# GCP Configuration
gcp_project_id = "globalbiting-dev"
gcp_region     = "us-central1"

# A2A Configuration
allowed_service_accounts_email = "ai-ap-service@globalbiting-dev.iam.gserviceaccount.com"
dev_nexus_url                  = "https://pattern-discovery-agent-665374072631.us-central1.run.app/"

# API Keys (SENSITIVE - use environment variables instead)
# Option 1: Set via environment variables:
#   export TF_VAR_gemini_api_key="your-gemini-key"
#   export TF_VAR_github_token="your-github-token"
#
# Option 2: Provide values below (NOT RECOMMENDED for production).
# These are placeholder/fake values so the example file is complete.
gemini_api_key = "fake-gemini-key-XXXXXXXXXXXXXXXX"
github_token   = "ghp_FAKEgithubtokenXXXXXXXXXXXXXXXXXXXX"

# Container Image (update after first Cloud Build deployment)
container_image = "gcr.io/globalbiting-dev/agentic-log-attacker:latest"

# Deployment Mode
# Set to false (default) if deploying via Cloud Build
# Set to true if you want Terraform to deploy the Cloud Run service directly
deploy_via_terraform = false

# Secret prefix to avoid collisions in Secret Manager
# Example will create secrets named like "agentic_log_attacker_GEMINI_API_KEY"
secret_prefix = "agentic_log_attacker"
